<?xml version="1.0" encoding="UTF-8"?>
<datasources xmlns="http://www.jboss.org/ironjacamar/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.jboss.org/ironjacamar/schema
	http://docs.jboss.org/ironjacamar/schema/datasources_2_0.xsd">
	<datasource jndi-name="java:jboss/datasources/mems" pool-name="mems" enabled="true" use-ccm="false">
		<connection-url>jdbc:postgresql://localhost:5432/mems_stagging</connection-url>
		<driver>postgresql</driver>
		<security>
			<user-name>postgres</user-name>
			<password>x</password>
		</security>
		<validation>
			<validate-on-match>false</validate-on-match>
			<background-validation>false</background-validation>
		</validation>
		<statement>
			<share-prepared-statements>false</share-prepared-statements>
		</statement>
	</datasource>
</datasources>